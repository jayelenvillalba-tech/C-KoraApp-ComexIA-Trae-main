{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "functions": {
    "api/index.ts": {
      "includeFiles": "comexia_v2.db"
    }
  },
  "typescript": {
    "ignoreBuildErrors": true
  }
}
